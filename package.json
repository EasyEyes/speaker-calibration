{
  "name": "speaker-calibration",
  "version": "2.2.252",
  "description": "Speaker calibration library for auditory testing",
  "main": "dist/main.js",
  "directories": {
    "bin": "dist"
  },
  "scripts": {
    "phrases": "node dist/example/fetch-languages-sheets.js && prettier --write dist/example/i18n.js",
    "build:prod": "webpack --mode production --progress",
    "build:dev": "webpack --watch --mode development --progress",
    "build:dev:analyze": "webpack --watch --mode development --progress --analyze",
    "serve:dev": "nodemon ./dist/example/server.js",
    "build:wasm": "make rebuild",
    "lint": "eslint src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "build:doc": "jsdoc src -r -d ./doc",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage"
  },
  "keywords": [],
  "constributors": [
    {
      "name": "Hugo De Moraes",
      "email": "demoraeshugo@gmail.com",
      "url": "https://github.com/demoraeshugo"
    }
  ],
  "license": "MIT",
  "repository": "git://github.com/EasyEyes/speaker-calibration.git",
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "axios": "^1.6.7",
    "fftw-js": "^0.1.4",
    "firebase": "^9.23.0",
    "googleapis": "^140.0.1",
    "np": "^7.7.0",
    "nvm": "^0.0.4",
    "path-browserify": "^1.0.1",
    "peerjs": "1.4.7",
    "qrcode": "^1.4.4",
    "socket.io-client": "^4.4.1",
    "sweetalert2": "^11.14.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-async-to-generator": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "babel-jest": "^27.5.1",
    "core-js": "^3.40.0",
    "css-loader": "^5.2.6",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.3.4",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "express": "^4.17.1",
    "jest": "^27.5.1",
    "jsdoc": "^4.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    "defaults"
  ]
}
